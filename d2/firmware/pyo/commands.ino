      expressArray = neutral;
      break;
    case 1: // joy
      expressArray = joy;
      break;
    case 2: // sadness
      expressArray = sadness;
      break;
    case 3: // anger
      expressArray = anger;
      break;
    case 4: // disgust
      expressArray = disgust;
      break;
    case 5: // fear
      expressArray = fear;
      break;
    case 6: // surprise
      expressArray = surprise;
      break;
  }

  for (uint16_t t = 0; t < 256; t++)
  {
    strip.setPixelColor(t, strip.Color(expressArray[t], expressArray[t], expressArray[t]));
  }

  strip.show();
}



void translateIR() // takes action based on IR code received

{
  switch (results.value) {

    ////////////////////////////////////////////////////////////////////////////// NEUTRAL _ button 0  /////////////////////////////////////////////////////////////////////////////////////////////////////
    case 0xFF9867:
      myDFPlayer.play(1);

      showEyes(0);

      //motor go to home position
      Mfront.write (0);
      Mright.write (0);
      Mleft.write (0);
      Mrotation.write (90);

      break;

    //////////////////////////////////////////////////////////////////////// JOY _ button 1 //////////////////////////////////////////////////////////////////////////////////////////////////////////

    case 0xFFA25D:


      showEyes(1);

      //voice
      myDFPlayer.play(2);
      delay(1200);


      //gesture
      for (pos = 0; pos <= 180; pos += 1) {   //move
        Mright.write(pos);
        Mleft.write(pos);
        delay(2);
      }

      for (pos = 180; pos >= 0; pos -= 1) { 
        Mright.write(pos);
        Mleft.write(pos);
        delay(2);
      }


      for (pos = 0; pos <= 120; pos += 1) {   //home
        Mfront.write(pos);
        delay(3);
      }

      for (pos = 120; pos >= 0; pos -= 1) { 
        Mfront.write(pos);
        delay(3);
      }

      delay (500);
      showEyes(0);

      break;

    ///////////////////////////////////////////////////////////////////////////// Sadness _ button 2 /////////////////////////////////////////////////////////////////////////////////////////

    case 0xFF629D:

      //voice
      myDFPlayer.play(3);
      delay (850);

      showEyes(2);

      //gesture
      for (pos = 0; pos <= 180; pos += 1) {   //move
        Mfront.write(pos);
        delay(7);
      }

      for (pos = 180; pos >= 0; pos -= 1) {   //home
        Mfront.write(pos);
        delay(7);
      }

      delay (500);
      showEyes(0);

      break;

    ////////////////////////////////////////////////////////////////////////////////// Anger _ button 3 /////////////////////////////////////////////////////////////////////////////////////////////////////////

    case 0xFFE21D:

      //voice
      myDFPlayer.play(4);
      delay (1300);

      showEyes(3);

      //gesture
      Mfront.write (90);
      delay(1000);
      Mfront.write (0);

      delay (500);
      showEyes(0);

      break;

    /////////////////////////////////////////////////////////////////////////////// Disgust _ button 4 //////////////////////////////////////////////////////////////////////////////////////

    case 0xFF22DD:

      //voice
      myDFPlayer.play(5);
      delay (750);

      showEyes(4);

      //gesture
      for (pos = 90; pos >= 60; pos -= 1) { 
        Mrotation.write(pos);
        delay(10);
      }

      for (pos = 0; pos <= 180; pos += 1) {
        Mleft.write(pos);
        delay(5);
      }

      for (pos = 60; pos <= 90; pos += 1) { 
        Mrotation.write(pos);
        delay(5);
      }
      Mleft.write (0);


      delay (500);
      showEyes(0);

      break;

    ///////////////////////////////////////////////////////////////////////////////// Fear _ button 5 //////////////////////////////////////////////////////////////////////////////////////////////////

    case 0xFF02FD:

      //voice
      myDFPlayer.play(6);
      delay (700);

      showEyes(5);

      //gesture
      for (pos = 0; pos <= 180; pos += 1) { 
        Mright.write(pos);
        Mleft.write(pos);
        delay(3);
      }
      delay(500);
      for (pos = 180; pos >= 0; pos -= 1) { 
        Mright.write(pos);
        Mleft.write(pos);
        delay(5);
      }

      delay (500);
      showEyes(0);

      break;

    //////////////////////////////////////////////////////////////////////////////// Surprise _ button 6 ///////////////////////////////////////////////////////////////////////////////////

    case 0xFFC23D:

      //voice
      myDFPlayer.play(7);
      delay (1200);

      showEyes(6);

      //gesture
      for (pos = 0; pos <= 120; pos += 1) { 
        Mright.write(pos);
        Mleft.write(pos);
        delay(2);
      }
      delay(500);

      Mright.write(0);
      Mleft.write(0);

      delay (500);
      showEyes(0);
      break;

  }// End Case


} //END translateIR
